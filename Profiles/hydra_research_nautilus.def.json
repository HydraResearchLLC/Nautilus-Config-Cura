		},
		"speed_travel": {
			"maximum_value_warning": "305",
			"default_value": 200,
			"value": "200"
		},
		"speed_layer_0": {
			"maximum_value_warning": "205",
			"default_value": 20,
			"value": "math.ceil(speed_print if speed_print < 20 else (((speed_volumetric_min / (line_width * layer_height))-20)/2) + 20 if (speed_volumetric_min / (line_width * layer_height)) > 20 else 20)"
		},
