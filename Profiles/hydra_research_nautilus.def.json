		},
		"speed_wall": {
			"maximum_value_warning": "205",
			"default_value": 25,
			"value": "math.ceil(speed_print if speed_print < 25 else (speed_volumetric_min / (line_width * layer_height)) if (speed_volumetric_min / (line_width * layer_height)) > 25 else 25)"
		},
		"speed_wall_0": {
			"maximum_value_warning": "205",
			"default_value": 25,
			"value": "math.ceil(speed_print if speed_print < speed_wall else speed_wall)"
		},
		"speed_wall_x": {
			"maximum_value_warning": "205",
			"default_value": 50,
			"value": "math.ceil(speed_print if speed_print < 50 else (speed_volumetric_min / (line_width * layer_height)) if (speed_volumetric_min / (line_width * layer_height)) > 50 else 50)"
		},
		"speed_roofing": {
			"maximum_value_warning": "205",
			"default_value": 50,
